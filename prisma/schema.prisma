generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model testimonials {
  id        Int       @id @default(autoincrement())
  rating    Int       @default(3)
  title     String?   @db.VarChar(255)
  content   String    @db.Text
  author    String    @db.VarChar(255)
}